{
    "name": "djohbot",
    "description": "djoh's random discord bot",
    "main": "build/src/sharding",
    "scripts": {
        "build:run": "npm run build && node --no-warnings --expose-gc .",
        "build": "npx tsc && npm run copy",
        "start": "node --no-warnings --expose-gc .",
        "minify": "node minify.js",
        "copy": "npx copyfiles \"src/**/*.!(ts)\" build",
        "watch": "npx tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/itzJOHv/djohbot.git"
    },
    "author": "djoh",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/itzJOHv/djohbot/issues"
    },
    "homepage": "https://github.com/itzJOHv/djohbot",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@eartharoid/i18n": "^1.0.4",
        "@fastify/cookie": "^8.0.0",
        "@fastify/session": "^9.0.0",
        "@fastify/view": "^7.1.0",
        "axios": "^0.27.2",
        "bottleneck": "github:SGrondin/bottleneck",
        "discord-api-types": "^0.37.5",
        "discord-hybrid-sharding": "^1.7.4",
        "discord-oauth2": "^2.10.0",
        "discord.js": "^14.3.0",
        "ejs": "^3.1.8",
        "erela.js": "github:menudocs/erela.js#build",
        "erela.js-spotify": "^1.2.0",
        "fastify": "^4.5.3",
        "lodash": "^4.17.21",
        "mongoose": "^6.5.4",
        "pretty-ms": "^7.0.1",
        "sanitize-filename": "^1.6.3",
        "string-progressbar": "^1.0.4",
        "winston": "^3.8.1",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/ejs": "^3.1.1",
        "@types/lodash": "^4.14.184",
        "@types/node": "^18.7.13",
        "recursive-readdir-synchronous": "^0.0.4",
        "terser": "^5.15.0",
        "typescript": "^4.8.2"
    }
}